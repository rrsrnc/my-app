{
    "install": {
      "commands": [
        "curl https://www.python.org/ftp/python/3.9.5/python-3.9.5-amd64.exe --output python-installer.exe",
        "python-installer.exe /quiet InstallAllUsers=1 PrependPath=1",
        "pip install scikit-learn"
      ]
    },
    "build": {
      "command": "npm run build"
    },
    "start": {
      "command": "npm start"
    }
  }
  